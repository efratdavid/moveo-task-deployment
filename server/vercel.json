  {
    "version": 2,
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 30
      }
    },
    "rewrites": [
      {
        "source": "/socket.io",
        "destination": "/api/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/api/index.js"
      }
    ]
  }