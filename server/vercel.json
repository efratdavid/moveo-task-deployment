  {
    "version": 2,
    "functions": {
      "index.js": {
        "runtime": "vercel-node@2.2.1",
        "maxDuration": 10
      }
    },
    "rewrites": [
      {
        "source": "/socket.io",
        "destination": "/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ]
  }